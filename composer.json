{
    "name": "mickadoo/basedata",
    "description": "A symfony command to ensure your database contains all base data required to use your application",
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "2.6.*",
        "doctrine/orm": "~2.2,>=2.2.3,<2.5",
        "doctrine/dbal": "<2.5",
        "doctrine/doctrine-bundle": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "autoload": {
        "psr-0": { "Mickadoo\\BaseDataBundle": "" }
    },
    "target-dir": "Mickadoo/BaseDataBundle",
    "authors": [
        {
            "name": "Michael Devery",
            "email": "michaeldevery@gmail.com"
        }
    ]
}
